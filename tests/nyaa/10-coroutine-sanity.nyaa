var t = new coroutine(lambda(a, b) {
	log(a, b)
	assert(a == 1)
	assert(b == 2)
	var c, d = yield(10, 11, 12)
	log(c, d)
	assert(c == 100)
	assert(d == 200)
	yield(-1, -2)
})

var a, b, c = t:resume(1, 2)
log(a, b, c)
assert(a == 10)
assert(b == 11)
assert(c == 12)
a, b = t:resume(100, 200)
assert(a == -1)
assert(b == -2)